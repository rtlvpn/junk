{
  "log": {
    "level": "warn",
    "timestamp": false
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "type": "https",
        "server": "1.1.1.1",
        "domain_resolver": "dns_resolver",
        "detour": "proxy"
      },
      {
        "tag": "dns_direct",
        "type": "https",
        "server": "1.1.1.1",
        "domain_resolver": "dns_resolver"
      },
      {
        "tag": "dns_resolver",
        "type": "udp",
        "server": "1.1.1.1"
      },
      {
        "tag": "dns_fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "query_type": ["CNAME","TXT"],
        "server": "dns_direct"
      },
        {
        "query_type": [
          "A",
          "AAAA",
          "SRV"
        ],
        "server": "dns_fakeip"
      },
      {
        "domain_suffix": [
          "usher.ttvnw.net",
          "playlist.ttvnw.net"
        ],
        "server": "dns_proxy"
      },
      
      {
        "rule_set": "geosite-ir",
        "server": "dns_resolver"
      }
    ],
    "final": "dns_proxy",
    "independent_cache": true,
    "strategy": "ipv4_only"
  },
  "route": {
    "rule_set": [
      {
        "tag": "geosite-category-ads-all",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ads-all.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-ir",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geosite-ir.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "block-httpdns",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/DDCHlsq/sing-ruleset/ruleset/httpdns.srs",
        "download_detour": "proxy"
      }
    ],
    "rules": [
        {
        "ip_cidr": [
          "185.206.94.87/32",
          "109.162.130.225/32", 
          "109.162.130.33/32",
          "109.230.223.74/32",
          "185.192.112.8/32",
          "188.121.100.200/32",
          "188.121.112.127/32"
        ],
        "outbound": "direct"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "action": "reject"
      },
      {
        "domain": [
          "passport.twitch.tv"
        ],
        "action": "route",
         "tls_record_fragment": true,
        "tls_fragment": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-ir",
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-ir",
        "invert": true,
        "outbound": "proxy"
      },
      {
        "domain": ["download.todesktop.com"],
        "action": "reject"
      },
      {
        "rule_set": "block-httpdns",
        "action": "reject"
      },
      {
        "rule_set": "geoip-cn",
        "outbound": "direct"
      },
      {
        "protocol": "ntp",
        "outbound": "proxy"
      },
      {
        "port": 123,
        "outbound": "proxy"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      }
    ],
    "final": "proxy",
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "dns_proxy"
    }
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "address": ["172.16.0.1/30", "fd00::1/126"],
      "mtu": 1492,
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "sniff": true,
      "sniff_override_destination": false
    },
    {
      "type": "mixed",
      "listen": "::",
      "listen_port": 2080
    }
  ],
  "outbounds": [
        {
      "type": "dnstt",
      "tag": "proxy",
      "domain": "t.godping.news",
      "pubkey": "68c16f27abfdfd59e6f97bb180ea67389d72a6bfb56f7c1862105f689e29f764",
      "udp_addrs": [
        "185.206.94.87:53",
        "109.162.130.225:53", 
        "109.162.130.33:53",
        "109.230.223.74:53",
        "185.192.112.8:53",
        "188.121.100.200:53",
        "185.192.112.8:53",
        "188.121.112.127:53"
      ],
      "local_addr": "127.0.0.1:8000",
      "network": ["tcp","udp"]
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "ntp": {
    "enabled": true,
    "server": "17.253.4.125",
    "server_port": 123,
    "interval": "30m"
  },
  "experimental": {
    "v2ray_api": {
      "listen": "127.0.0.1:8080",
      "stats": {
        "enabled": true,
        "inbounds": ["mixed-in", "tun-in"],
        "outbounds": ["direct", "proxy"]
      }
    },
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_fakeip": true
    }
  }
}
